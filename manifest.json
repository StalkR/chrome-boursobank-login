{
  "manifest_version": 2,
  "name": "Boursorama Login",
  "version": "1.6",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [ {
    "exclude_globs": [],
    "exclude_matches": [],
    "include_globs": [],
    "js": [
      "resemble.js",
      "settings.js",
      "script.js"
    ],
    "matches": [
      "https://clients.boursorama.com/connexion/"
    ],
    "run_at": "document_idle"
  } ]
}
